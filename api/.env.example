# MongoDB connection (used by Docker Compose — override for external MongoDB)
MONGODB_URI=mongodb://mongo:27017/palette-db

# Admin API token (required for /admin/* endpoints)
ADMIN_TOKEN=change-me-to-a-secure-random-string

# Restrict admin CORS origin (optional — defaults to * if unset)
ADMIN_ORIGIN=https://mkbabb.github.io

# Server port (default 3000)
PORT=3000
